<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:common="urn:dom.gosuslugi.ru/common/1.2.0" xmlns:tns="urn:dom.gosuslugi.ru/accruals-payments-egisso/1.0.0" targetNamespace="urn:dom.gosuslugi.ru/accruals-payments-egisso/1.0.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="urn:dom.gosuslugi.ru/common/1.2.0" schemaLocation="./commons/dom-gosuslugi-ru-smev3-common.xsd"/>
	<xs:simpleType name="InformationTypeType">
		<xs:annotation>
			<xs:documentation>Тип, описывающий тип сведений. Возможные значения: ACCRUALS - информация о начислениях; PAYMENTS - информация о произведенных платежах.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="ACCRUALS"/>
			<xs:enumeration value="PAYMENTS"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PaymentDocumentNumberType">
		<xs:annotation>
			<xs:documentation>Тип, описывающий номер платежного документа</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="30"/>
			<xs:pattern value="(.*)([0-9а-яА-Яa-zA-Z]+)(.*)"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="UnifiedAccountNumberType">
		<xs:annotation>
			<xs:documentation>Тип, описывающий номер единого лицевого счета</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AccountNumberType">
		<xs:annotation>
			<xs:documentation>Тип, описывающий номер лицевого счета/иной идентификатор потребителя</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="64"/>
			<xs:pattern value="(.*)([0-9а-яА-Яa-zA-Z]+)(.*)"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ServiceIDType">
		<xs:annotation>
			<xs:documentation>Тип, описывающий идентификатор жилищно-коммунальной услуги (ИЖКУ)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="13"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PaymentDocumentIDType">
		<xs:annotation>
			<xs:documentation>Тип, описывающий идентификатор платежного документа</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="18"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AreaType">
		<xs:annotation>
			<xs:documentation>Тип, описывающий площадь</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="0"/>
			<xs:totalDigits value="34"/>
			<xs:fractionDigits value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="String255Type">
		<xs:annotation>
			<xs:documentation>Тип, описывающий строку не более 255 символов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="255"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="ExportAccrualsPaymentsRequest">
		<xs:annotation>
			<xs:documentation>Корневой элемент запроса экспорта информации о начислениях, о произведенных платежах и характеристиках объектов жилищного фонда</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="common:BaseRequestType">
					<xs:sequence>
						<xs:element name="information-type" type="tns:InformationTypeType">
							<xs:annotation>
								<xs:documentation>Тип сведений. Возможные значения: 
ACCRUALS - информация о начислениях; 
PAYMENTS - информация о произведенных платежах.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:choice>
							<xs:element name="housing-fund-object" maxOccurs="100">
								<xs:annotation>
									<xs:documentation>Информация об объекте жилищного фонда</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:choice>
										<xs:element name="apartment-house">
											<xs:annotation>
												<xs:documentation>Помещение или комната в многоквартирном доме</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:choice>
													<xs:element name="premise-guid" type="common:UUID">
														<xs:annotation>
															<xs:documentation>Глобальный уникальный идентификатор помещения</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="room-guid" type="common:UUID">
														<xs:annotation>
															<xs:documentation>Глобальный уникальный идентификатор комнаты</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:choice>
											</xs:complexType>
										</xs:element>
										<xs:element name="living-house">
											<xs:annotation>
												<xs:documentation>Жилой дом или комната в жилом доме</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:choice>
													<xs:element name="fias-house-code" type="common:UUID">
														<xs:annotation>
															<xs:documentation>Глобальный уникальный идентификатор дома по ФИАС / Идентификационный код дома в ГИС ЖКХ</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="hm-house-guid" type="common:UUID">
														<xs:annotation>
															<xs:documentation>Глобальный уникальный идентификатор дома (РОЖФ)</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="room-guid" type="common:UUID">
														<xs:annotation>
															<xs:documentation>Глобальный уникальный идентификатор комнаты</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:choice>
											</xs:complexType>
										</xs:element>
										<xs:element name="living-block-house">
											<xs:annotation>
												<xs:documentation>Блок или комната в жилом доме блокированной застройки</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:choice>
													<xs:element name="premise-guid" type="common:UUID">
														<xs:annotation>
															<xs:documentation>Глобальный уникальный идентификатор помещения/блока</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="room-guid" type="common:UUID">
														<xs:annotation>
															<xs:documentation>Глобальный уникальный идентификатор комнаты</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:choice>
											</xs:complexType>
										</xs:element>
									</xs:choice>
								</xs:complexType>
							</xs:element>
							<xs:element name="unified-account-number" type="tns:UnifiedAccountNumberType" maxOccurs="100">
								<xs:annotation>
									<xs:documentation>Единый лицевой счет</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:choice>
						<xs:element name="period">
							<xs:annotation>
								<xs:documentation>Период, за который запрашиваются сведения. Период ограничен шестью календарными месяцами.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="start-date" type="xs:gYearMonth">
										<xs:annotation>
											<xs:documentation>Дата начала периода </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="end-date" type="xs:gYearMonth">
										<xs:annotation>
											<xs:documentation>Дата окончание периода </xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExportAccrualsPaymentsResponse">
		<xs:annotation>
			<xs:documentation>Корневой элемент ответа на запрос экспорта информации о начислениях, о произведенных платежах и характеристиках объектов жилищного фонда</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:sequence minOccurs="0">
					<xs:element name="exported-to-ftp" type="xs:boolean" fixed="true">
						<xs:annotation>
							<xs:documentation>Результат запроса выложен на FTP</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="create-date" type="xs:dateTime">
						<xs:annotation>
							<xs:documentation>Дата и время формирования файла, содержащего информацию о начислениях, о произведенных платежах и характеристиках объектов жилищного фонда</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:element name="error" type="common:ErrorType" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Список ошибок, возникших при обработке запроса</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExportAccrualsPaymentsPackage">
		<xs:annotation>
			<xs:documentation>Корневой элемент выгрузки информации о начислениях, о произведенных платежах и характеристиках объектов жилищного фонда, выгружаемый на FTP</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="version" type="xs:string" fixed="1.0.0">
					<xs:annotation>
						<xs:documentation>Версия формата выгрузки</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="create-date" type="xs:dateTime">
					<xs:annotation>
						<xs:documentation>Дата выгрузки</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="house" type="tns:HouseType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Данные о доме</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="ExportAccrualType">
		<xs:annotation>
			<xs:documentation>Тип, описывающий выгружаемую информация о начислениях</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="payment-document" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Платежный документ</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="payment-document-guid" type="common:UUID">
							<xs:annotation>
								<xs:documentation>GUID платежного документа </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="payment-document-id" type="tns:PaymentDocumentIDType">
							<xs:annotation>
								<xs:documentation>Идентификатор платежного документа</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="payment-document-number" type="tns:PaymentDocumentNumberType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Номер платежного документа</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="service-id" type="tns:ServiceIDType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Идентификатор жилищно-коммунальной услуги (ИЖКУ)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="account-number" type="tns:AccountNumberType">
							<xs:annotation>
								<xs:documentation>Номер лицевого счета/Иной идентификтатор потребителя</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="account-status" type="tns:AccountStatusType">
							<xs:annotation>
								<xs:documentation>Статус лицевого счета ACTUAL/CLOSED</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="period" type="xs:gYearMonth">
							<xs:annotation>
								<xs:documentation>Расчетный период</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="invoice-date" type="xs:date">
							<xs:annotation>
								<xs:documentation>Дата выставления платежного документа</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="total-amount" type="xs:decimal">
							<xs:annotation>
								<xs:documentation>Общая сумма начисленных денежных средств для внесения платы
за содержание и ремонт жилого помещения и предоставление
коммунальных услуг (ресурсов) в запрашиваемом периоде</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="use-total-amount-with-debt-and-advance" type="xs:boolean" fixed="true" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Итоговая сумма по платежному документу указана с учетом суммы задолженности</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="total-amount-with-debt-and-advance" type="xs:decimal" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Итоговая сумма по платежному документу указана с учетом суммы задолженности</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="service-provider" type="tns:AccrualServiceProviderType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Cумма начисленных денежных средств 
разрезе поставщиков услуг (ресурсов)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="service" type="tns:AccrualServiceType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Cумма начисленных денежных средств 
разрезе услуг (ресурсов)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ExportPaymentsType">
		<xs:annotation>
			<xs:documentation>Тип, описывающий выгружаемую информация об извещениях о принятии к исполнению распоряжения</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="notification" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Извещения о приятии к распоряжению</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="notification-guid" type="common:UUID">
							<xs:annotation>
								<xs:documentation>Идентификатор извещения о приятии к распоряжению</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="order-no" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Номер распоряжения</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="25"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="order-id" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Уникальный идентификатор распоряжения</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="32"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="payment-date" type="xs:date" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Дата платежа</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="total" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Сумма платежа</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:decimal">
									<xs:totalDigits value="18"/>
									<xs:fractionDigits value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="total-ack" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Сумма квитирования</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:decimal">
									<xs:totalDigits value="18"/>
									<xs:fractionDigits value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="hcs-service-id" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Идентификатор ЖКУ (ИЖКУ)</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="13"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="payment-document-id" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Идентификатор платежного документа (ИДПД)</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="18"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="payment-document-number" type="tns:PaymentDocumentNumberType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Номер платежного документа</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="payment-period" type="xs:gYearMonth" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Период оплаты</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="fias-house-code" type="common:UUID" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Глобальный уникальный идентификатор дома по ФИАС</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="account-number" type="tns:AccountNumberType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Номер лицевого счета/Иной идентификтатор потребителя</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="flat-number" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Номер жилого помещения</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="255"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="room-number" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Номер комнаты</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="255"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="non-living-number" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Номер нежилого помещения</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="255"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="address-string" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Неформализованное описание адреса</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="provider-name" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Наименование получателя платежа</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="provider-inn" type="common:INNType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>ИНН фактического получателя платежа</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="provider-kpp" type="common:KPPType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>КПП фактического получателя платежа</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="payment-document" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Информация по оплаченным/подобранным начислениям (платежным документам) по извещению</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="payment-document-guid" type="common:UUID">
