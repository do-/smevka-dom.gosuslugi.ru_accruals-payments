	<xs:complexType name="DiffCriterionType">
		<xs:annotation>
			<xs:documentation>Тип, описывающий критерии дифференциации</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="diff-criterion-guid" type="common:UUID">
				<xs:annotation>
					<xs:documentation>Идентификатор критерия дифференциации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type">
				<xs:annotation>
					<xs:documentation>Тип критерия дифференциаци</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="operator">
				<xs:annotation>
					<xs:documentation>Оператор критерия дифференциации</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="source" type="tns:DiffCriterionSource">
				<xs:annotation>
					<xs:documentation>Источник критерия дифференциации. Возможные значения: COMMON; SPECIAL_ATTRIBUTE. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="criterion-code">
				<xs:annotation>
					<xs:documentation>Код критерия дифференциации. Источник определяется по source</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="criterion-name">
				<xs:annotation>
					<xs:documentation>Наименование критерия дифференциации</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="500"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="logical-value" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Значение критерия (признак). Заполняется если type=BOOLEAN</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="integer-value-from" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Значение критерия от (целое число). Заполняется если type=INTEGER или YEAR</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="integer-value-to" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Значение критерия до (целое число). Заполняется если type=INTEGER или YEAR</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="real-value-to" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Значение критерия до (вещественное число). Заполняется если type=REAL</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="real-value-from" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Значение критерия от (вещественное число). Заполняется если type=REAL</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="other-diff-criterion" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Иной критерий дифференциации. Заполняется если type=MULTILINE</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="value">
							<xs:annotation>
								<xs:documentation>Значение</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="2000"/>
									<xs:minLength value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="name">
							<xs:annotation>
								<xs:documentation>Наименование</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="500"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="diff-criterion-reference-object" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Значение критерия - справочник НСИ. Заполняется если type=ENUMERABLE</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="reference-code" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Код справочника</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="3"/>
									<xs:minLength value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="nsi-code">
							<xs:annotation>
								<xs:documentation>Справочное значение.Код</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255"/>
									<xs:minLength value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="nsi-name" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Справочное значение.Наименование</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="2000"/>
									<xs:minLength value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="source">
							<xs:annotation>
								<xs:documentation>Источник справочного значения</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="50"/>
									<xs:minLength value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="diff-criterion-fias-address" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Критерий дифференциации - адрес ФИАС. Заполняется если type=FIAS</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice>
						<xs:element name="fias-address-object-code" type="common:UUID">
							<xs:annotation>
								<xs:documentation>Идентификационный код адресного объекта (улицы, населенного пункта, города, района) в ГИС ЖКХ</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="fias-house-code" type="common:UUID">
							<xs:annotation>
								<xs:documentation>Идентификационный код дома в ГИС ЖКХ</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="DiffCriterionSource">
		<xs:annotation>
			<xs:documentation>Тип, описывающий источник критерия дифференциации</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="COMMON"/>
			<xs:enumeration value="SPECIAL_ATTRIBUTE"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="AccrualServiceType">
		<xs:annotation>
			<xs:documentation>Тип, описывающий услугу в информации о начислениях</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="guid" type="common:UUID">
				<xs:annotation>
					<xs:documentation>Идентификатор услуги</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="receiver-guid" type="common:UUID">
				<xs:annotation>
					<xs:documentation>Идентификатор получателя платежа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="service-type" type="tns:ServiceTypeType">
				<xs:annotation>
					<xs:documentation>Условное обозначение типа услуги</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="charge-type" type="common:NSIRefType">
				<xs:annotation>
					<xs:documentation>Услуга</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="total-amount" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Итого к оплате</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="amount-by-service" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>К оплате за расчетный период, рублей</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tariff" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Тариф/размер платы/размер взноса</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="individual-consumption-norm" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Норматив потребления</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="okei" minOccurs="0">
				<xs:annotation>
					<xs:documentation>ЕИ услуги</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="code">
							<xs:annotation>
								<xs:documentation>ЕИ услуги (Код записи справочника ОКЕИ)</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:pattern value="\d{3}"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="name">
							<xs:annotation>
								<xs:documentation>ЕИ услуги (Наименование записи справочника ОКЕИ)</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="500"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="volume" minOccurs="0" maxOccurs="2">
				<xs:annotation>
					<xs:documentation>Объем потребления по услуге. Два объема может быть для отопления (индивидуальное и общедомовое)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="type" type="tns:ConsumptionTypeType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Тип:
(I)ndividualConsumption - объем коммунальных услуг
house(O)verallNeeds - объем коммунальных ресурсов, потребленных при использовании и содержании общего имущества в многоквартирном доме, или коммунальных услуг на общедомовые нужды</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="consumption-measure-type" type="tns:ConsumptionMeasureTypeType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Способ определения объемов КУ:
(N)orm - Норматив
(M)etering device - Прибор учета
(O)ther - Иное</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="consumption" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Объем услуги (ресурса)</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:decimal">
									<xs:totalDigits value="22"/>
									<xs:fractionDigits value="7"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PaymentServiceType">
		<xs:annotation>
			<xs:documentation>Тип, описывающий услугу в информации об извещениях о принятии к исполнению распоряжения</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="total-amount" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>Сумма оплаты</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="service-type">
				<xs:annotation>
					<xs:documentation>Услгуга. Указываются записи из следующих справочников:
№1 "Дополнительные услуги"
№51 "Коммунальные услуги"
№337 "Коммунальные ресурсы, потребляемые при использовании и содержании общего имущества в многоквартирном доме"
№59 "Работы и услуги организации"</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="common:NSIRefType">
							<xs:sequence>
								<xs:element name="nsi-ref-guid" type="common:UUID">
									<xs:annotation>
										<xs:documentation>Уникалный идентификатор справочника НСИ. Може содержать идентификатор одного из следующих справочников:
№1 "Дополнительные услуги"
№51 "Коммунальные услуги"
№337 "Коммунальные ресурсы, потребляемые при использовании и содержании общего имущества в многоквартирном доме"
№59 "Работы и услуги организации"</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="period" type="common:MonthYearType">
				<xs:annotation>
					<xs:documentation>Период</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="HouseType">
		<xs:annotation>
			<xs:documentation>Тип, описывающий сведения по дому</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="fias-house-code" type="common:UUID">
